{
    "title": "Invocation manifest for OpenSlide to PNG file converter",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "config": {
            "type": "object",
            "properties": {
                "gear-log-level": {
                    "default": "INFO",
                    "type": "string",
                    "enum": [
                        "ERROR",
                        "WARNING",
                        "INFO",
                        "DEBUG"
                    ]
                }
            },
            "required": [
                "gear-log-level"
            ]
        },
        "inputs": {
            "type": "object",
            "properties": {
                "image": {
                    "type": "object",
                    "properties": {}
                }
            },
            "required": [
                "image"
            ]
        }
    },
    "required": [
        "config",
        "inputs"
    ]
}
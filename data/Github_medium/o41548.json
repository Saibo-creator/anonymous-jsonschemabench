{
    "title": "Invocation manifest for CNI: GE P-File Classifier",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "config": {
            "type": "object",
            "properties": {
                "timezone": {
                    "type": "string",
                    "default": "America/Los_Angeles"
                }
            },
            "required": [
                "timezone"
            ]
        },
        "inputs": {
            "type": "object",
            "properties": {
                "pfile": {
                    "type": "object",
                    "properties": {
                        "type": {
                            "enum": [
                                "pfile"
                            ]
                        }
                    }
                }
            },
            "required": [
                "pfile"
            ]
        }
    },
    "required": [
        "config",
        "inputs"
    ]
}
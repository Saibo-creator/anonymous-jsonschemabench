{
    "description": "NousResearch/json-mode-eval 95",
    "schema": {
        "title": "DockingTimeQuery",
        "type": "object",
        "properties": {
            "portName": {
                "title": "Port Name",
                "type": "string"
            },
            "date": {
                "title": "Date",
                "type": "string",
                "format": "date"
            },
            "timeSlots": {
                "title": "Time Slots",
                "type": "array",
                "items": {
                    "type": "string",
                    "pattern": "^([01]?[0-9]|2[0-3]):[0-5][0-9]$"
                }
            }
        },
        "required": [
            "portName",
            "date",
            "timeSlots"
        ]
    },
    "tests": [
